{
	"name": "open-utilities",
	"version": "1.0.0",
	"description": "A library of common design patterns.",
	"type": "module",
	"exports": {
		"./core": {
			"import": "./dst/core/index.js"
		},
		"./async": {
			"import": "./dst/async/index.js"
		},
		"./geometry": {
			"import": "./dst/geometry/index.js"
		},
		"./rendering-web": {
			"import": "./dst/rendering-web/index.js"
		},
		"./ui": {
			"import": "./dst/ui/index.js"
		}
	},
	"typesVersions": {
		"*": {
			"core": ["./dst/core/index.d.ts"],
			"async": ["./dst/async/index.d.ts"],
			"geometry": ["./dst/geometry/index.d.ts"],
			"rendering-web": ["./dst/rendering-web/index.d.ts"],
			"ui": ["./dst/ui/index.d.ts"]
		}
	},
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"watch": "tsc -p tsconfig.json -w"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/TheCymaera/open-utilities.git"
	},
	"keywords": [],
